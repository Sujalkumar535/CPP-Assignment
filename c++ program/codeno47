#include<iostream>
using namespace std;
class largest
{
    private:
    int a,b;
    public:
    void getdata()
    {
        cout<<"enter two number";
        cin>>a>>b;
    }
    void check()
    {
        if(a>b)
            cout<<"largest number is"<<a;
        else
            cout<<"largest number is"<<b;
    }
};
int main()
{
    largest l;
    l.getdata();
    l.check();
    return 0;
}